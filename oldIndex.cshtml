@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">

    @foreach (var item in ViewBag.listaClientes)
    {
        <div>@item.Id</div>
        <div>@item.Nome</div>
        <div>@item.CPF</div>
    }


    @foreach (var item in ViewBag.listaFornecedores)
    {
        <div>@item.Id</div>
        <div>@item.Nome</div>
        <div>@item.CNPJ</div>
    }

</div>
